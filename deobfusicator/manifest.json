{
    "manifest_version": 3,
    "name": "Code Deobfuscator",
    "version": "1.0",
    "description": "Deobfuscates JavaScript and PHP code on webpages",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_title": "Deobfuscate Code",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }